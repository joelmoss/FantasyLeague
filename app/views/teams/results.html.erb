<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th width="110">Date</th>
      <th></th>
      <th></th>
      <th width="10">Score</th>
      <th></th>
      <th class="text-right">Players</th>
      <%- FixturePlayer::METRICS.each do |k,v| -%>
        <%- next if k == :pld -%>
        <th class="text-right"><%= k.to_s.upcase %></th>
      <%- end -%>
    </tr>
  </thead>
  <tbody>
    <%= render partial: 'teams/result', collection: @results, as: :fixture %>
  </tbody>
</table>